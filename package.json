{
  "name": "radzima.app",
  "version": "1.0.0",
  "description": "",
  "main": ".eleventy.js",
  "scripts": {
    "build:assets": "webpack --config webpack.config.prod.js",
    "build:site": "cross-env ELEVENTY_ENV=production npx eleventy",
    "del:assets": "rimraf ./src/compiled-assets",
    "del:dist": "rimraf ./dist",
    "build": "webpack --config webpack.config.dev.js",
    "server": "cross-env ELEVENTY_SERVE=true ELEVENTY_ENV=development npx eleventy --serve",
    "serve:prod": "serve ./dist/",
    "dev": "concurrently \"npm run server\" \"npm run build\"",
    "prod": "npm-run-all del:dist del:assets build:assets build:site"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@babel/core": "7.18.10",
    "@babel/preset-env": "7.18.10",
    "@google-cloud/storage": "^6.4.2",
    "autoprefixer": "10.4.8",
    "axios": "^0.27.2",
    "babel-loader": "8.2.5",
    "bluebird": "^3.7.2",
    "concurrently": "^7.3.0",
    "core-js": "3.24.1",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "css-loader": "5.2.7",
    "css-minimizer-webpack-plugin": "4.0.0",
    "csv-parse": "^5.3.0",
    "dotenv": "^16.0.2",
    "eslint": "8.22.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.26.0",
    "google-auth-library": "^8.5.1",
    "googleapis": "^107.0.0",
    "html-minifier": "4.0.0",
    "jsonwebtoken": "^8.5.1",
    "md5-file": "5.0.0",
    "mini-css-extract-plugin": "2.7.2",
    "npm-run-all": "4.1.5",
    "postcss-loader": "^4.0.1",
    "rimraf": "3.0.2",
    "sass": "1.54.4",
    "sass-loader": "10.0.2",
    "serve": "14.0.1",
    "terser-webpack-plugin": "4.2.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "5.8.0"
  }
}
