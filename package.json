{
  "name": "radzima.app",
  "version": "1.0.0",
  "description": "",
  "main": ".eleventy.js",
  "scripts": {
    "build:assets": "webpack --config webpack.config.prod.js",
    "build:site": "cross-env ELEVENTY_ENV=production npx eleventy",
    "del:assets": "rimraf ./src/compiled-assets",
    "del:dist": "rimraf ./dist",
    "build": "webpack --config webpack.config.dev.js",
    "server": "cross-env ELEVENTY_ENV=development npx eleventy --serve",
    "serve:prod": "serve ./dist/",
    "dev": "concurrently \"npm run server\" \"npm run build\"",
    "prod": "npm-run-all del:dist del:assets build:assets build:site"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@babel/core": "7.11.6",
    "@babel/preset-env": "7.11.5",
    "autoprefixer": "9.8.6",
    "babel-loader": "8.1.0",
    "concurrently": "^7.3.0",
    "core-js": "3.6.5",
    "cross-env": "^7.0.3",
    "css-loader": "4.2.2",
    "eslint": "7.8.1",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.22.0",
    "html-minifier": "4.0.0",
    "md5-file": "5.0.0",
    "mini-css-extract-plugin": "0.11.0",
    "npm-run-all": "4.1.5",
    "optimize-css-assets-webpack-plugin": "6.0.1",
    "postcss-loader": "4.0.1",
    "rimraf": "3.0.2",
    "sass": "1.54.0",
    "sass-loader": "10.0.2",
    "serve": "11.3.2",
    "terser-webpack-plugin": "4.1.0",
    "webpack": "4.46.0",
    "webpack-cli": "3.3.12",
    "webpack-merge": "5.8.0"
  }
}
